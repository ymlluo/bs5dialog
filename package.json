{
  "name": "bs5dialog",
  "version": "0.0.3",
  "description": "bs5dialog is a library for creating Bootstrap 5 dialogs. It provides a set of components for creating alerts, prompts, confirms, messages, toasts, tabs, spinners, and offcanvas dialogs.",
  "main": "dist/bs5dialog.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "type": "module",
  "keywords": [
    "bootstrap",
    "bootstrap5",
    "modal",
    "dialog",
    "loading",
    "alert",
    "confirm",
    "prompt",
    "toast",
    "offcanvas",
    "modal"
  ],
  "author": "ymlluo",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-modules": "^6.0.0",
    "postcss-modules-values": "^4.0.0",
    "rollup": "3.20.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-import-css": "^3.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-watch": "^1.0.2"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "bootstrap": "5.3.0-alpha1",
    "spinkit": "^2.0.1"
  }
}

